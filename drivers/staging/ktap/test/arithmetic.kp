#!/usr/bin/env ktap

function failed() {
	printf("failed\n");
	exit(-1);
}

#-----------------------------------------#

if (1 > 2) {
	failed()
}

if (200 < 100) {
	failed()
}

a = 4
b = 5

if ((a + b) != 9) {
	failed()
}

if ((a - b) != -1) {
	failed()
}

if ((a % b) != 4) {
	failed()
}

if ((a / b) != 0) {
	failed()
}


#below checking only valid for 64-bit system

c = 0x1234567812345678
d = 0x2

if (c + d != 0x123456781234567a) {
	failed()
}

if (-1 != 0xffffffffffffffff) {
	failed()
}

